                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ProjectExists" minOccurs="0">
                      <xs:complexType>
                        <xs:attribute name="href" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ProjectSelect" minOccurs="0">
                      <xs:complexType>
                        <xs:attribute name="href" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="ConfirmSelection" minOccurs="0" type="xs:string" />
                    <xs:element name="AssertOperationSuccessful" minOccurs="0" type="xs:string" />
