                        <xs:attribute name="name" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="FindTextBox" minOccurs="0">
                      <xs:complexType>
                        <xs:attribute name="name" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="TypeText" minOccurs="0">
                      <xs:complexType>
                        <xs:attribute name="searchText" type="xs:string" />
